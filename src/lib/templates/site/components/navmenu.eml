    [%eml {|<a class="button<% ( if (String.equal name current) then (%> current<%)) ; %>" href="<%-url%>"><%-name%></a>|}] in %_>
<section id="navmenu">
    <_%- navmenu_item "Index" "/" %_>
    <_%- navmenu_item "Resume" "/resume_en" %_>
</section>

